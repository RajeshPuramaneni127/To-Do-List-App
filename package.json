        "cross-env": "^7.0",
        "jquery": "^3.6.0",
        "laravel-mix": "^6.0.40",
        "lodash": "^4.17.21",
        "popper.js": "^1.16.1",
        "resolve-url-loader": "^3.1.4",
        "sass": "^1.41.0",
        "sass-loader": "^12.1.0",
        "vue": "^2.7.14",
        "vue-loader": "^15.9.8",
        "vue-template-compiler": "^2.6.14",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.8.0"
